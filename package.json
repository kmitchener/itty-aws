{
  "name": "itty-aws",
  "version": "0.1.0",
  "files": [
    "dist"
  ],
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -b",
    "generate": "bun scripts/generate-clients.ts && bun biome check --write && tsc -b",
    "generate-ec2-metadata": "bun scripts/generate-ec2-metadata.ts && bun biome check --write && tsc -b",
    "generate-service-support-table": "bun scripts/generate-service-support-table.ts",
    "gen": "bun generate",
    "bundle:size": "bun scripts/bundle-size.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "aws4fetch": "^1.0.20",
    "fast-xml-parser": "^4.4.0"
  },
  "peerDependencies": {
    "@aws-sdk/credential-providers": "^3",
    "@effect/platform": "^0.87.12",
    "@effect/platform-node": "^0.89.5",
    "@smithy/node-config-provider": "^4",
    "effect": "^3"
  },
  "devDependencies": {
    "@aws-sdk/credential-providers": "^3",
    "@biomejs/biome": "^2.1.1",
    "@effect/platform-node": "^0.89.5",
    "@effect/platform": "^0.87.12",
    "@effect/vitest": "^0.23.12",
    "@smithy/node-config-provider": "^4",
    "@types/bun": "^1.2.18",
    "@types/node": "^20",
    "bun-types": "^1.2.18",
    "effect": "^3.16.12",
    "esbuild": "^0.19.0",
    "typescript": "^5.8.3",
    "vitest": "3"
  }
}
